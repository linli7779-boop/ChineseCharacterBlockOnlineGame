{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/levels/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/api/idioms/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/game.js",
      "dest": "app.py"
    },
    {
      "src": "/style.css",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}

